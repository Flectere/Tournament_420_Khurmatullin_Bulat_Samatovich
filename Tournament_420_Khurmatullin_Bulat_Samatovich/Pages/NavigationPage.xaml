<Page x:Class="Tournament_420_Khurmatullin_Bulat_Samatovich.Pages.NavigationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Tournament_420_Khurmatullin_Bulat_Samatovich.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NavigationPage">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <TextBlock Text="Выберите роль" Grid.Column="1"/>
        <Button Width="150" Height="100" Name="RegBt" Grid.Row="1" Grid.Column="0" Content="Регистрация" Click="RegBt_Click"/>
        <Button Width="150" Height="100" Name="LoginBt" Grid.Row="1" Grid.Column="1" Content="Авторизация" Click="LoginBt_Click"/>
    </Grid>
</Page>
